/ {
    conditional_layers {
        compatible = "zmk,conditional-layers";
        // NONE (0, 0000) is non-conditional
        // A (1, 1000) is non-conditional
        // B (2, 0100) is non-conditional
        ab {     // 1100
            if-layers = <2 3>;
            then-layer = <4>;
        };
        // C (4, 0010) is non-conditional
        ac {    // 1010
            if-layers = <2 5>;
            then-layer = <6>;
        };
        bc {    // 0110
            if-layers = <3 5>;
            then-layer = <7>;
        };
        abc {   // 1110
            if-layers = <2 3 5>;
            then-layer = <8>;
        };
        // D (8, 0001) is non-conditional
        ad {    // 1001
            if-layers = <2 9>;
            then-layer = <10>;
        };
        bd {    // 0101
            if-layers = <3 9>;
            then-layer = <11>;
        };
        abd {   // 1101
            if-layers = <2 3 9>;
            then-layer = <12>;
        };
        cd {    // 0011
            if-layers = <5 9>;
            then-layer = <13>;
        };
        acd {   // 1011
            if-layers = <2 5 9>;
            then-layer = <14>;
        };
        bcd {   // 0111
            if-layers = <3 5 9>;
            then-layer = <15>;
        };
        abcd {  // 1111
            if-layers = <2 3 5 9>;
            then-layer = <16>;
        };
        win_lin_a_default_behavior {
            if-layers = <2>;
            then-layer = <2>;
        };
        win_lin_a {
            if-layers = <2 17>;
            then-layer = <18>;
        };
    };

    macros {
        undo_winlin: undo_winlin {
            label = "undo_winlin";
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings
                = <&macro_press &kp LCTRL>
                , <&macro_tap &kp Z>
                , <&macro_release &kp LCTRL>
                ;
        };

        undo_osx: undo_osx {
