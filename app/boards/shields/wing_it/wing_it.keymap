     macros {
        pair_mac: pair_mac {
            label = "Pair_Mac";
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&bt BT_SEL 1>, <&metaset 1>, <&ext_power EP_OFF> ;
        };
        pair_lin: pair_lin {
            label = "Pair_Linux";
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&bt BT_SEL 0>, <&metaset 0>, <&ext_power EP_OFF> ;
        };
        quLower: quLower {
            label = "QuLower";
            compatible = "zmk,behavior-macro";
            wait-ms = <15>;
            tap-ms = <15>;
            #binding-cells = <0>;
            bindings = <&kp Q>,
                       <&kp U>;
        };
        quCapital: quCapital {
            label = "QuCaptital";
            compatible = "zmk,behavior-macro";
            wait-ms = <35>;
            tap-ms = <25>;
            #binding-cells = <0>;
            bindings = <&macro_press &kp LSHFT>,
                       <&macro_tap &kp Q>,
                       <&macro_release &kp LSHFT>,
                       <&macro_tap &kp U>;
        };
    };


    behaviors {
        tdNavSym: tap_dance_nav_sym {
            compatible = "zmk,behavior-tap-dance";
            label = "TAP_DANCE_NAV_SYM";
            #binding-cells = <0>;
            tapping-term-ms = <500>;
            bindings = <&mo 1>, <&mo 2>;    //hold = navigation, tap+hold = symbols
        };
        tdSymNav: tap_dance_sym_nav {
            compatible = "zmk,behavior-tap-dance";
            label = "TAP_DANCE_SYM_NAV";
            #binding-cells = <0>;
            tapping-term-ms = <500>;
            bindings = <&mo 2>, <&mo 1>;    //hold = symbols, tap+hold = navigation
        };
        tdFnNum: tap_dance_fn_num {
            compatible = "zmk,behavior-tap-dance";
            label = "TAP_DANCE_FN_NUM";
            #binding-cells = <0>;
            tapping-term-ms = <500>;
            bindings = <&mo 3>, <&mo 4>;    //hold = functions, tap+hold = numbers
        };
        tdNumFn: tap_dance_num_fn {
            compatible = "zmk,behavior-tap-dance";
            label = "TAP_DANCE_NUM_FN";
            #binding-cells = <0>;
            tapping-term-ms = <500>;
            bindings = <&mo 4>, <&mo 3>;    //hold = numbers, tap+hold = functions
        };
        tdMediaSys: tap_dance_media_sys {
            compatible = "zmk,behavior-tap-dance";
            label = "TAP_DANCE_MEDIA_SYS";
            #binding-cells = <0>;
            tapping-term-ms = <500>;
            bindings = <&mo 5>, <&mo 6>;    //hold = media, tap+hold = system
        };
        qu: qu {
            compatible = "zmk,behavior-mod-morph";
            label = "QU";
            #binding-cells = <0>;
            bindings = <&quLower>, <&quCapital>;
            mods = <(MOD_LSFT|MOD_RSFT)>;
        };
        tdQu: tap_dance_qu {
            compatible = "zmk,behavior-tap-dance";
            label = "TAP_DANCE_QU";
            #binding-cells = <0>;
            tapping-term-ms = <140>;
            bindings = <&qu>, <&kp Q>;
        };
    };

    keymap {
        compatible = "zmk,keymap";

        default_layer {
/* Carpalx QGMLWY
    Left Hand                                                 L. Thumb                              R. Thumb    Right Hand
    ┌────────┬────────┬────────┬────────┬────────┬────────┐                                                    ┌────────┬────────┬────────┬────────┬────────┬────────┐
    │  ESC   │    Q   │   W    │   E    │   R    │   T    │                                                    │    Y   │   U    │   I    │    O   │   P    │ BKSPC  │
    ├────────┼────────┼────────┼────────┼────────┼────────┤                                                    ├────────┼────────┼────────┼────────┼────────┼────────┤
    │  TAB   │    A   │   S    │   D    │   F    │   G    |                                                    │    H   │   J    │   K    │    L   │   ;    │ ENTER  │
    ├────────┼────────┼────────┼────────┼────────┼────────┤                                                    ├────────┼────────┼────────┼────────┼────────┼────────┤
    │  SHIFT │    Z   │   X    │   C    │   V    │   B    │                                                    │    N   │   M    │   , <  │  . >   │   / ?  │ R SHFT │
    ├────────┼────────┼────────┼────────┼────────┼────────┤  ┌────────┐┌────────┐        ┌────────┐┌────────┐  ├────────┼────────┼────────┼────────┼────────┼────────┤
    │  CTL   │  GUI   │  ALT   │  FN 8  │  FN 2  │  SPACE │  │  FN 1  ││  FN 4  │        │  FN 8  ││  FN 2  │  │ SPACE  │  FN 1  │  FN 4  │  ALT   │  GUI   │  CTL   │
    └────────┴────────┴────────┴────────┴────────┴────────┘  └────────┘└────────┘        └────────┘└────────┘  └────────┴────────┴────────┴────────┴────────┴────────┘

 */
                        bindings = <
&kp  ESC        &tdQu            &kp  G       &kp  M            &kp  L    &kp  W                                               &kp  Y        &kp  F   &kp  U           &kp  B        &kp  SEMI       &kp  BSPC
&kp  TAB        &kp  D           &kp  S       &kp  T            &kp  N    &kp  R                                               &kp  I        &kp  A   &kp  E           &kp  O        &kp  H          &kp  ENTER
&meta LALT LCMD &kp  Z           &kp  X       &kp  C            &kp  V    &kp  J        &mo  5                     &mo  9      &kp  K        &kp  P   &kp  COMMA       &kp  DOT      &kp  SLASH      &meta RALT RCMD
&kp  LCTRL      &meta LGUI LALT  &tdMediaSys  &meta LCTRL LCMD  &tdNumFn  &kp  LSHIFT   &tdNavSym                  &tdSymNav   &kp  SPACE    &tdFnNum &meta RCTRL RCMD &tdMediaSys   &meta RGUI RALT &kp  RCTRL
            >;
        };


        navigation {  // 1
/* Navigation
    ┌────────┬─────────┬─────────┬─────────┬─────────┬─────────┐                                                    ┌────────┬─────────┬─────────┬─────────┬─────────┬────────┐
    │        │ shift+` │backspace│   UP    │   DEL   │ctl/alt+K│                                                    │        │   HOME  │         │         │scrnshot │        │
    ├────────┼─────────┼─────────┼─────────┼─────────┼─────────┤                                                    ├────────┼─────────┼─────────┼─────────┼─────────┼────────┤
    │        |shift+tab│   left  │  down   │  right  │ctl/alt+J|                                                    │        │   END   │ alt/cmd │ctrl/alt │         │        │
    ├────────┼─────────┼─────────┼─────────┼─────────┼─────────┤                                                    ├────────┼─────────┼─────────┼─────────┼─────────┼────────┤
    │        │         │   CUT   │  COPY   │  PASTE  │         │                                                    │        │         │         │         │         │        │
    └────────┴─────────┼─────────┼─────────┼─────────┼─────────┤  ┌────────┐                            ┌────────┐  ├────────┼─────────┼─────────┼─────────┼─────────┴────────┘
                       │Media/Sys│ctrl/cmd │Func/num │  Shift  │  │Nav./sym│                            │Sym/nav.│  │  Space │Num/func │ctrl/cmd |Media/sys│
                       └─────────┴─────────┴─────────┴─────────┘  └────────┘                            └────────┘  └────────┴─────────┴─────────┴─────────┘
 */
                        bindings = <
&trans   &kp LS(GRAVE)  &kp BSPC           &kp UP             &kp DEL            &meta LC(K) LA(K)                                  &none   &kp HOME &none            &none            &meta PSCRN LC(LG(LS(N4)))  &trans
&trans   &kp  LS(TAB)   &kp LEFT           &kp DOWN           &kp RIGHT          &meta LC(J) LA(J)                                  &none   &kp END  &meta LALT LGUI  &meta LCTL LALT  &kp LSHIFT                  &trans
&trans   &none          &meta LC(X) LG(X)  &meta LC(C) LG(C)  &meta LC(V) LG(V)  &none               &trans                 &trans  &none   &none    &none            &none            &none                       &trans
&trans   &trans         &trans             &trans             &trans             &trans              &trans                 &trans  &trans  &trans   &trans           &trans           &trans                      &trans
            >;
        };

        symbols {  // 2
/* Symbols
